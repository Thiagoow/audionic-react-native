{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@app/*": ["./app/*"],
      "@Assets/*": ["./assets/*"],
      "@Components*": ["./components/*"],
      "@Data/*": ["./data/*"],
      "@Navigators/*": ["./navigators/*"],
      "@Screens/*": ["./screens/*"],
      "@Constants/*": ["./constants/*"],
      "@Utils/*": ["./utils/*"]
    }
  }
}
