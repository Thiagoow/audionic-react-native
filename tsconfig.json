{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@Assets/*": ["src/assets/*"],
      "@Components*": ["src/components/*"],
      "@Data/*": ["src/data/*"],
      "@Navigators/*": ["src/navigators/*"],
      "@Screens/*": ["src/screens/*"],
      "@Theme/*": ["src/theme/*"],
      "@Utils/*": ["src/utils/*"]
    }
  }
}
